        </activity>
        <activity
            android:name=".Carga_secciones"
            android:label="@string/title_activity_carga_secciones" >
        </activity>
        <activity
            android:name=".Carga_notas_seccion"
            android:label="@string/title_activity_carga_notas_seccion" >
